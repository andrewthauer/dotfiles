{
  "assistant": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4o"
    },
    "button": true,
    "version": "2"
  },
  "autosave": "off",
  "buffer_font_size": 15,
  "buffer_font_family": "FiraCode Nerd Font Mono",
  "ensure_final_newline_on_save": true,
  "features": {
    "copilot": true
  },
  "format_on_save": "off",
  "languages": {
    "Ruby": {
      "language_servers": ["ruby-lsp", "!solargraph", "rubocop", "..."]
    }
  },
  "lsp": {},
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "font_family": "FiraCode Nerd Font Mono",
    "font_size": 15,
    "option_as_meta": true,
    "shell": "system"
  },
  "theme": "Catppuccin Frapp√©",
  "ui_font_family": "FiraCode Nerd Font Propo",
  "ui_font_size": 15,
  "vim_mode": true
}
